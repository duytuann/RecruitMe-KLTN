{
  "name": "recruit-me",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-admin": "vite --port 5001 --mode admin",
    "dev-employer": "vite --port 5002 --mode employer",
    "dev-jobseeker": "vite --port 5003 --mode jobseeker",
    "build-admin": "vite build --mode admin",
    "build-employer": "vite build --mode employer",
    "build-jobseeker": "vite build --mode jobseeker",
    "dev": "concurrently \"npm run dev-admin\"  \"npm run dev-jobseeker\" \"npm run dev-employer\"",
    "build": "concurrently \"npm run dev-admin\"  \"npm run build-jobseeker\" \"npm run build-employer\"",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "antd": "^5.10.3",
    "axios": "^1.6.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "history": "^5.3.0",
    "less": "^4.2.0",
    "postcss": "^8.4.31",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.5"
  }
}
